{
  "type": "module",
  "scripts": {
    "dev": "node server",
    "serve": "serve ../docs",
    "build": "npm-run-all clean:* setup:* build:*",
    "clean:public": "rimraf public",
    "clean:dist": "rimraf dist",
    "clean:docs": "rimraf ../docs",
    "setup:public": "mkdir public && cp --recursive r/*/ public",
    "setup:contents": "node contents",
    "build:client": "vite build",
    "build:server": "vite build --ssr",
    "build:prerender": "vite-plugin-ssr prerender --noExtraDir",
    "build:copy": "cp --recursive dist/client ../docs"
  },
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "@preact/preset-vite": "^2.2.0",
    "express": "^4.18.1",
    "html-react-parser": "^1.4.14",
    "jsdom": "^19.0.0",
    "npm-run-all": "^4.1.5",
    "preact": "^10.7.3",
    "preact-render-to-string": "^5.2.0",
    "react-collapsible": "^2.9.0",
    "rimraf": "^3.0.2",
    "sass": "^1.52.1",
    "vite": "^2.9.9",
    "vite-plugin-full-reload": "^1.0.0",
    "vite-plugin-ssr": "^0.3.64"
  }
}
